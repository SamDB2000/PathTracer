{
  "version": "2.0.0",
  "windows": {
    "options": {
      "shell": {
        "executable": "cmd.exe",
        "args": [
          "/C",
          "\"C:/Program Files (x86)/Microsoft Visual Studio/2019/Community/Common7/Tools/VsDevCmd.bat\"",
          "&&"
        ]
      }
    }
  },
  "tasks": [
    {
      "type": "shell",
      "label": "msbuild (Debug)",
      "command": "msbuild.exe",
      "args": [
        "/m"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "msbuild (Release)",
      "command": "msbuild.exe",
      "args": [
        "/property:Configuration=Release",
        "/m"
      ],
      "group": "build",
      "problemMatcher": []
    }
  ]
}
